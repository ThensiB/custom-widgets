{
	"id": "com.GitHub.box",
	"version": "1.0.0",
	"name": "Demo Gauge",
	"description": "A gauge demo",
	"newInstancePrefix": "Gauge",
	"icon": "https://github.com/ThensiB/custom-widgets/blob/master/icon.png",
	"vendor": "GitHub",
	"eula": "EULA",
	"license": "1.0",
	"webcomponents": [
		{
			"kind": "main",
			"tag": "com-GitHub-box",
			"url": "https://github.com/ThensiB/custom-widgets/blob/master/box.js",
			"integrity": "sha 384-61bb34c30e94b8464caa79b433b6c4ed57d20dd55e8ac3e82089ec1fcde1590d12f35d7a2a194b0010abaf9b8e763c25",
			"ignoreIntegrity": true
		},
		{
			"kind": "styling",
			"tag": "com-GitHub-box-sps",
			"url": "https://github.com/ThensiB/custom-widgets/blob/master/box_sps.js",
			"integrity": "sha 384-a4baac457605ad5045c14c3c3ce6f478f8b1925968ff6d960e7cb715e1250c3c5241da359b0d83d214b76cfc6f736614",
			"ignoreIntegrity": true
			
		},
		{
			"kind": "builder",
			"tag": "com-GitHub-box-bps",
			"url": "https://github.com/ThensiB/custom-widgets/blob/master/box_bps.js",
			"integrity": "sha 384-381d60015e8bdac5acc350db78bd6db0e48a3f8e249f3212de0d60d2ffb743051c86fabb070a99eb919c3f8c3f42bff2",
			"ignoreIntegrity": true
			
			
		}
	],
	"properties": {
		"value": {
			"type": "number",
			"description": "Gauge value",
			"default": 0
		},
		"info": {
			"type": "string",
			"description": "Gauge info",
			"default": ""
		},
		"color": {
			"type": "string",
			"description": "Text Color info",
			"default": "#3498db"
		},
		"width": {
			"type": "integer",
			"default": 100
		},
		"height": {
			"type": "integer",
			"default": 100
		}
	},
	"methods": {
		"setValue": {
			"description": "Sets the Gauge value.",
			"parameters": [
				{
					"name": "newValue",
					"type": "number",
					"description": "Gauge value"
				},
				{
					"name": "newInfo",
					"type": "string",
					"description": "Gauge info"
				},
				{
					"name": "newColor",
					"type": "string",
					"description": "Text Color info"
				}
			],
			"body": "this.value = newValue; this.info = newInfo; this.color = newColor;"
		},
		"getValue": {
			"returnType": "number",
			"description": "Returns the Gauge value.",
			"body": "return this.value;"
		}
	},
	"events": {
		"onClick": {
			"description": "Called when the user clicks the Box."
		}
	}
}


 