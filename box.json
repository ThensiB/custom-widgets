{
	"id": "com.GitHub.box",
	"version": "1.0.0",
	"name": "Demo Gauge",
	"description": "A gauge demo",
	"newInstancePrefix": "Gauge",
	"icon": "https://github.com/ThensiB/custom-widgets/blob/master/icon.png",
	"vendor": "GitHub",
	"eula": "EULA",
	"license": "1.0",
	"webcomponents": [
		{
			"kind": "main",
			"tag": "com-GitHub-box",
			"url": "https://github.com/ThensiB/custom-widgets/blob/master/box.js",
			"integrity": "sha256-181d9660bb6cd6c31c65ebe604dce90e53ee66698b4514ba0e5849b9599837f0",
			"ignoreIntegrity": false
		},
		{
			"kind": "styling",
			"tag": "com-GitHub-box-sps",
			"url": "https://github.com/ThensiB/custom-widgets/blob/master/box_sps.js",
			"integrity": "sha256-8999620c54733c03ec7d1b8ac6ac7de956718682959d83327f50b240a66a9bab",
			"ignoreIntegrity": false
			
		},
		{
			"kind": "builder",
			"tag": "com-GitHub-box-bps",
			"url": "https://github.com/ThensiB/custom-widgets/blob/master/box_bps.js",
			"integrity": "sha256-dd4f2fb0c9ac8e3fa9c46f97a36f8819a69c88ac9ddf13a6e79906e920c0f1b7",
			"ignoreIntegrity": false
			
			
		}
	],
	"properties": {
		"value": {
			"type": "number",
			"description": "Gauge value",
			"default": 0
		},
		"info": {
			"type": "string",
			"description": "Gauge info",
			"default": ""
		},
		"color": {
			"type": "string",
			"description": "Text Color info",
			"default": "#3498db"
		},
		"width": {
			"type": "integer",
			"default": 100
		},
		"height": {
			"type": "integer",
			"default": 100
		}
	},
	"methods": {
		"setValue": {
			"description": "Sets the Gauge value.",
			"parameters": [
				{
					"name": "newValue",
					"type": "number",
					"description": "Gauge value"
				},
				{
					"name": "newInfo",
					"type": "string",
					"description": "Gauge info"
				},
				{
					"name": "newColor",
					"type": "string",
					"description": "Text Color info"
				}
			],
			"body": "this.value = newValue; this.info = newInfo; this.color = newColor;"
		},
		"getValue": {
			"returnType": "number",
			"description": "Returns the Gauge value.",
			"body": "return this.value;"
		}
	},
	"events": {
		"onClick": {
			"description": "Called when the user clicks the Box."
		}
	}
}


 
